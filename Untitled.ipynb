   "source": [
    "#１２音階を求められる順に並べてみました\n",
    "i = 1\n",
    "gosa = 0.01\n",
    "print(i)\n",
    "i *= 3\n",
    "while i > 2:\n",
    "    i /= 2\n",
    "    if i < 2 - gosa:\n",
    "        print(i)\n",
    "        i *= 3\n",
    "    elif abs(2 - i) <= gosa:\n",
    "        print(i)\n",
    "        break"
   ]
